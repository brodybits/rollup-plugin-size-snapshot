{
  "name": "@brodybits/rollup-plugin-size-snapshot",
  "version": "0.7.2-dev",
  "main": "dist/index.js",
  "files": [
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "build": "babel src --out-dir dist",
    "lint-check": "prettier --check --end-of-line=auto --ignore-path=.gitignore ./**/*.{js,md}",
    "test-build": "rm -rf temp && mkdir -p temp && cp -r tests temp && babel src --out-dir temp/src && jest temp/tests",
    "test": "jest && yarn lint-check",
    "precommit": "lint-staged",
    "prepublish": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write"
    ]
  },
  "babel": {
    "presets": [
      "@babel/flow",
      [
        "@babel/env",
        {
          "targets": {
            "node": "10"
          }
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": "https://github.com/brodybits/rollup-plugin-size-snapshot",
  "author": "Bogdan Chadkin <trysound@yandex.ru>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-flow": "^7.16.5",
    "husky": "^4.3.8",
    "jest": "^27.4.5",
    "jest-matcher-deep-close-to": "^3.0.2",
    "lint-staged": "^10.5.4",
    "prettier": "~1.19.1",
    "redux": "^4.1.2",
    "rollup": "^0.65.2",
    "rollup-plugin-terser": "^2.0.2",
    "strip-ansi": "^4.0.0"
  },
  "peerDependencies": {
    "rollup": ">=0.65.0 <1"
  },
  "dependencies": {
    "acorn": "^6.0.1",
    "bytes": "^3.0.0",
    "chalk": "^2.4.1",
    "gzip-size": "^5.0.0",
    "jest-diff": "^26.6.2",
    "memory-fs": "^0.5.0",
    "rollup-plugin-replace": "^2.0.0",
    "terser": "^3.8.2",
    "webpack": "^5.65.0"
  },
  "engines": {
    "node": ">=10"
  }
}
